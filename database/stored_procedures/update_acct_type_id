-- --------------------------------------------------------------------------------
-- Routine DDL
-- Note: comments before and after the routine body will not be stored by the server
-- --------------------------------------------------------------------------------
DELIMITER $$

CREATE DEFINER=`root`@`localhost` PROCEDURE `update_acct_type_id`(IN account_id INT)
BEGIN
	UPDATE election_officer
	SET acct_type_id = 4
	WHERE acct_id = account_id;
END